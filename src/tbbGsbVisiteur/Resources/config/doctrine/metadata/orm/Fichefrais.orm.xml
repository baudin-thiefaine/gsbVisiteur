<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Fichefrais" table="fichefrais">
    <indexes>
      <index name="idEtat" columns="idEtat"/>
      <index name="idVisiteur" columns="idVisiteur"/>
    </indexes>
    <id name="idfichefrais" type="integer" column="idFicheFrais">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mois" type="string" column="mois" length="128" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="annee" type="integer" column="annee" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbjustificatifs" type="integer" column="nbJustificatifs" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="montantvalide" type="decimal" column="montantValide" precision="10" scale="2" nullable="true"/>
    <field name="datemodif" type="date" column="dateModif" nullable="true"/>
    <many-to-one field="idetat" target-entity="Etat" fetch="LAZY">
      <join-columns>
        <join-column name="idEtat" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idvisiteur" target-entity="Visiteur" fetch="LAZY">
      <join-columns>
        <join-column name="idVisiteur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idfraisforfait" target-entity="Fraisforfait" inversed-by="idfichefrais" fetch="LAZY">
      <join-table name="lignefraisforfait">
        <join-columns>
          <join-column name="idFicheFrais" referenced-column-name="idFicheFrais"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="idFraisForfait" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
